datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}

model users {
  id        String   @id @default(uuid())
  email     String
  password  String
  name      String
  phone     String?
  avatar    String?
  gender    String?
  createdAt DateTime @default(now())
}
